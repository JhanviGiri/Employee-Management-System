    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
          
        try{
 
            String id=d_tf.getText();
            Class.forName("com.mysql.cj.jdbc.Driver");
            Connection cn=DriverManager.getConnection("jdbc:mysql://localhost:3306/Database_Name","Username","Password");
            
            PreparedStatement ps=cn.prepareStatement("DELETE FROM employee WHERE id=?");
            ps.setString(1, id);
            boolean b=ps.execute();
            if(b==false){
                JOptionPane.showMessageDialog(rootPane, "Successfully Deleted");
            }
            cn.close();
        }
        catch(Exception e){
            System.out.println(e.getMessage());
    }
    }                                        

    private void jButton8ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        try{
       
     Class.forName("com.mysql.cj.jdbc.Driver");
            Connection cn=DriverManager.getConnection("jdbc:mysql://localhost:3306/Database_Name","Username","Password");
           
            PreparedStatement ps=cn.prepareStatement("select * from employee");
           
           
           
           ResultSet rs=ps.executeQuery();
            while(rs.next())
            {
            String id=rs.getString(1);
            String name=rs.getString(2);
            String email=rs.getString(3);
            String mobile=rs.getString(4);
            String salary=rs.getString(5);
           
            String s[]={id,name,email,mobile,salary};
             DefaultTableModel dtm=(DefaultTableModel)table.getModel();
            dtm.addRow(s);
            }
           
           
           
            cn.close();
        }
        catch(Exception e){
            System.out.println(e.getMessage());
           
           
        }
    }                                        

