    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        
try{


String name=tf1.getText();
String email=tf2.getText();
String mobile=tf3.getText();
String salary=tf4.getText();

if(mobile.length()!=10)
{
JOptionPane.showMessageDialog(rootPane, "Invalid mobile");
System.exit(0);
}

if (!email.contains("@") || !email.contains(".")) {
    JOptionPane.showMessageDialog(rootPane, "Invalid email");
    System.exit(0);
}


Class.forName("com.mysql.cj.jdbc.Driver");
Connection cn=DriverManager.getConnection("jdbc:mysql://localhost:3306/Database_Name","Username","Password");

PreparedStatement ps=cn.prepareStatement("insert into employee(name,email,mobile,salary)values(?,?,?,?)");
ps.setString(1, name);
ps.setString(2, email);
ps.setString(3, mobile);
ps.setString(4, salary);


boolean b=ps.execute();
if(b==false){
JOptionPane.showMessageDialog(rootPane, "Successfully Registered");
}
cn.close();
}
catch(Exception e){
System.out.println(e.getMessage());
}
    }                                        

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
                                     
try{
    
        String id=s_tf.getText();
        Class.forName("com.mysql.cj.jdbc.Driver");
            Connection cn=DriverManager.getConnection("jdbc:mysql://localhost:3306/Database_Name","Username","Password");
            
            PreparedStatement ps=cn.prepareStatement("select * from employee where id=?");
            ps.setString(1, id);
            

            ResultSet rs=ps.executeQuery();
            if(rs.next())
            {
            String name=rs.getString(2);
            String email=rs.getString(3);
            String mobile=rs.getString(4);
            String salary=rs.getString(5);
            
            l_name.setText(name);
            l_email.setText(email);
            l_mobile.setText(mobile);
            l_salary.setText(salary);
            
            }
          cn.close();
        }
        catch(Exception e){
            System.out.println(e.getMessage());
           
        }
    
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
                                    
try{
        String id=e_id.getText();
     Class.forName("com.mysql.cj.jdbc.Driver");
            Connection cn=DriverManager.getConnection("jdbc:mysql://localhost:3306/Database_Name","Username","Password");
            
            PreparedStatement ps=cn.prepareStatement("select * from employee where id=?");
            ps.setString(1, id);
            
            
           ResultSet rs=ps.executeQuery();
            if(rs.next())
            {
            String name=rs.getString(2);
            String email=rs.getString(3);
            String mobile=rs.getString(4);
            String salary=rs.getString(5);
            
            t_name.setText(name);
            t_email.setText(email);
